{
    "name": "juanparati/mybackup",
    "description": "A MySQL/MariaDB backup tool",
    "keywords": ["mysql", "mariadb", "backup", "console", "tool", "encrypt"],
    "homepage": "https://github.com/mybackup",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Juan Lago",
            "email": "juanparati@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2.0",
        "ext-openssl": "*",
        "ext-pdo": "*",
        "ext-zlib": "*",
        "illuminate/database": "^11.5",
        "laravel-zero/framework": "^11.0.0",
        "laravel/slack-notification-channel": "^3.2",
        "league/flysystem-aws-s3-v3": "^3.0",
        "league/flysystem-ftp": "^3.0",
        "league/flysystem-google-cloud-storage": "^3.0",
        "league/flysystem-sftp-v3": "^3.0",
        "spatie/laravel-google-cloud-storage": "^2.3",
        "symfony/process": "^7.0",
        "symfony/yaml": "^7.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23",
        "laravel/pint": "^1.15.2",
        "mockery/mockery": "^1.6.11",
        "pestphp/pest": "^2.34.7"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "bin": ["application"]
}
